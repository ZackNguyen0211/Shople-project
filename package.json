{
  "name": "shoople-mono",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "apps/web"
  ],
  "scripts": {
    "build": "npm run build -w @shoople/web",
    "dev": "npm run dev -w @shoople/web",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs",
    "vercel-build": "npm run build -w @shoople/web",
    "seed-images": "node seed-product-images.js",
    "cleanup-images": "node cleanup-storage.js",
    "seed-images:category": "node seed-product-images-by-category.js",
    "reseed-images": "npm run cleanup-images && npm run seed-images:category",
    "download-images": "node download-product-images.js"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.8",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  }
}
